
public class Demo {

	public void function1()
	{
		System.out.println("Welcome to function-1");
	}
	public void function2()
	{
		System.out.println("Welcome to function-2");
	}
	
	public static void main(String[] args) {
		
		System.out.println("This Main Function");
		
		Demo d1=new Demo();
		d1.function1();
		d1.function2();

	}

}
